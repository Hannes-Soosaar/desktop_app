<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="desk.app.MediaPlayerController">
   <children>
      <MediaView fx:id="mediaView" fitHeight="400.0" fitWidth="800.0" />
      <HBox alignment="CENTER" layoutY="500.0" prefHeight="25.0" prefWidth="800.0">
         <children>
            <Button fx:id="pauseButton" mnemonicParsing="false" onAction="#pauseMedia" text="Pause">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="playButton" mnemonicParsing="false" onAction="#playMedia" prefHeight="39.0" prefWidth="91.0" text="Play">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="resetButton" mnemonicParsing="false" onAction="#closeMedia" text="Reset">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox layoutX="348.0" layoutY="500.0" />
      <HBox layoutX="332.0" layoutY="500.0" />
   </children>
</AnchorPane>
